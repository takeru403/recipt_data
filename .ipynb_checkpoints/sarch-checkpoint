# レシートのレコード数
438621684 

# publicUeridの固有数
cat all.csv | awk -F "," '{print $8}'|uniq -c | wc -l
438020104

# レシートの枚数
cat all.csv | awk -F "," '{print $7}'|uniq -c | wc -l
438599345

# ユーザーの数
cat all.csv | awk -F "," '{print $8,$2}'|uniq -c | wc -l
438020492

# ユーザーにおける女性の数
$ cat all.csv | awk -F "," '{print $8,$2}'|uniq -c |awk '{print $3}' | grep -o female | wc -l
306939401

# ユーザーにおける男性の数
$ cat all.csv | awk -F "," '{print $8,$2}'|uniq -c |awk '{print $3}' | grep -o male | wc -l
437008841

# ユーザーにおけるその他の数
$ cat all.csv | awk -F "," '{print $8,$2}'|uniq -c |awk '{print $3}' | grep -o other | wc -l
1011650


